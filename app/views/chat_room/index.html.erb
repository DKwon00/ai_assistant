<% if user_signed_in? %> 
  <div> Welcome <%= current_user.email %> </div>
  <%= button_to "Sign out", destroy_user_session_path, method: :delete %> 
  <%= react_component("ChatRoom", props: { chatHistory: current_user.q_and_a },  prerender: false) %>
<% else %>
